{
    "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "strictPropertyInitialization": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
  